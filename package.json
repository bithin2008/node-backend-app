    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/app.js",
    "clean": "rm -rf node_modules",
    "migrate-dev": "set NODE_ENV=development && sequelize db:migrate --config src/config/config.js",
    "migrate-staging": "set NODE_ENV=staging && npx sequelize db:migrate --config src/config/config.js",
    "migrate": "node -e \"require('./src/migration').migrate()\"",
    "migrate:rollback": "node -e \"require('./src/migration').migrateRollback()\"",
    "migrate:reset": "node -e \"require('./src/migration').migrateReset()\"",
    "start:dev": "NODE_ENV=development  nodemon src/app.js",
    "start:staging": "NODE_ENV=staging nodemon src/app.js",
    "start:uat": "set NODE_ENV=uat && nodemon src/app.js"
  },
