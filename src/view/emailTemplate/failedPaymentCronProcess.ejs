	<style type="text/css">
		* {
			font-family: 'verdana', sans-serif;
		}

		@media print {
			thead {
				display: table-header-group;
			}

			tfoot {
				display: table-footer-group;
			}

			tr {
				page-break-inside: avoid;
			}

			@page {
				size: A4;
			}

			.page-break {
				display: block;
				page-break-after: always;
			}
		}
		body{
            position: relative;
        }
        body:before{
            content: '';
            background-color: #d8962a;
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            z-index: -1;
            height: 17%;
        }
	</style>
</head>

