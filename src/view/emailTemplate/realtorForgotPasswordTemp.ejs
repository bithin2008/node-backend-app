			}

			tr {
				page-break-inside: avoid;
			}

			@page {
				size: A4;
			}

			.page-break {
				display: block;
				page-break-after: always;
			}
		}
		body{
            position: relative;
        }
        body:before{
            content: '';
            background-color: #364a77;
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            z-index: -1;
            height: 17%;
        }
